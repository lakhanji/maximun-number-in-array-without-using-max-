#include<bits/stdc++.h>
using namespace std;



int maxim(int num[], int n){
    int maxi= INT_MIN;
    for(int i=0;i<n;i++){
        if(maxi>num[i])
        maxi = num[i];

    }
return maxi;
}


int main(){
    int n;
    cin>>n;
    int num[100];
    
    for(int i=0; i<=n; i++){
        cin>>num[i];
        
    }
    cout<<"maximum value= "<<maxim(num,n)<<endl;
    mimim(num,n);
    
return 0;
}
